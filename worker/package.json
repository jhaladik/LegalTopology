{
  "name": "legal-topology-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker for Czech Legal Topology - Multi-layer legal reasoning system",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:dev": "wrangler deploy --env development",
    "deploy:prod": "wrangler deploy --env production",
    "tail": "wrangler tail",
    "secret:openai": "wrangler secret put OPENAI_API_KEY",
    "vectorize:create": "wrangler vectorize create czech-legal-topology --dimensions=1536 --metric=cosine",
    "r2:create": "wrangler r2 bucket create legal-documents"
  },
  "keywords": [
    "legal",
    "topology",
    "vectorize",
    "cloudflare-workers",
    "czech-law"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "mammoth": "^1.11.0",
    "unpdf": "^1.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "typescript": "^5.6.2",
    "wrangler": "^3.78.0"
  }
}
